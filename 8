x = ''
for i in range(1, 10):
    k = x + str(i)
    s = int(k) * 8 + i
    print(k, "* 8 +", i, "=", s)
    x = k

x = ''
for i in range(1, 10):
    k = x + str(i)
    s = int(k) * 9 + i + 1
    print(k, "* 9 +", i + 1, "=", s)
    x = k

x = ''
for i in range(0, 10):
    k = x + str(i)
    s = int(k) * 9 + i + 1
    print(s,'*', s, "=", s ** 2)
    x=k
    
